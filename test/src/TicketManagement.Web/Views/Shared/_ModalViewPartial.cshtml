@model IEnumerable<string>
<link href="~/css/main.css" rel="stylesheet">
<div class="modal-body">
    <div class="modal-header">
        <h2>@ViewData["Header"]</h2>
    </div>
    <div>
        @if (@Model?.Count() > 0)
        {
            @foreach (var message in @Model)
            {
                <h4>@message</h4>
            }
        }
    </div>
</div>
